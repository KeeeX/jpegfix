{
  "name": "@keeex/jpegfix",
  "version": "1.0.2",
  "description": "Recovery tool to retrieve picture data from damaged jpeg files",
  "main": "lib/services/jpeg.js",
  "scripts": {
    "build": "tslib build",
    "check": "tslib check",
    "clean": "tslib clean",
    "doc": "tslib typedoc",
    "prepack": "npm run clean && npm run build",
    "start": "npm run build && bin/jpegfix.js",
    "test": "tslib test"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/KeeeX/jpegfix.git"
  },
  "keywords": [
    "jpeg",
    "fix",
    "cli",
    "recovery"
  ],
  "author": "KeeeX SAS",
  "contributors": [
    {
      "name": "Gabriel Paul \"Cley Faye\" Risterucci",
      "email": "gabriel@keeex.net"
    }
  ],
  "bin": "bin/jpegfix.js",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KeeeX/jpegfix/issues"
  },
  "homepage": "https://github.com/KeeeX/jpegfix#readme",
  "devDependencies": {
    "@keeex/eslint-config": "^1.11.5",
    "@keeex/tslib": "^1.16.0",
    "@swc/core": "^1.3.96",
    "@types/argparse": "^2.0.13",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.18.63",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "c8": "^8.0.1",
    "eslint": "^8.53.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "knip": "^2.41.2",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.3",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "argparse": "^2.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "tslib": {
    "knip": {
      "entry": [
        "bin/jpegfix.ts",
        "services/jpeg.ts"
      ],
      "ignore": [
        "tests/**/*"
      ],
      "ignoreDependencies": [
        "@types/mocha"
      ],
      "ignoreBinaries": [
        "bin/jpegfix.js"
      ]
    }
  },
  "c8": {
    "exclude": [
      "src/bin/jpegfix.ts"
    ]
  }
}
